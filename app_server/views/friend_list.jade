extends layout

block content
  div
    h1= title
    p #{message} #{title}
    input(type="hidden", value=nouser, id="nu")
    
    div.container
     .row
     .col-xs-12.col-md-6
      .row.list-group
      each f in friends
        .list-group-item            
          a(href="/friend/"+f._id)= f.username           
